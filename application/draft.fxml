                     </children>
                  </AnchorPane>
                  <AnchorPane layoutX="145.0" layoutY="83.0" prefHeight="317.0" prefWidth="343.0">
                     <children>
                        <StackedBarChart fx:id="chart" layoutX="15.0" layoutY="34.0" prefHeight="246.0" prefWidth="328.0">
                          <xAxis>
                            <CategoryAxis side="BOTTOM" />
                          </xAxis>
                          <yAxis>
                            <NumberAxis side="LEFT" />
                          </yAxis>
                        </StackedBarChart>
                        <TextField fx:id="presentValue" editable="false" layoutX="128.0" layoutY="275.0" mouseTransparent="true" />

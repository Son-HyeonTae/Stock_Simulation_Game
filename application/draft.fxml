                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="270.0" prefWidth="136.0">
                                 <children>
                                    <ListView fx:id="stocklist" layoutX="-1.0" layoutY="-1.0" prefHeight="474.0" prefWidth="178.0" />
                                 </children>
                              </AnchorPane>
                          </content>
                        </ScrollPane>
                     </children>
                  </AnchorPane>
               </children>
            </Pane>
         </children>
    </AnchorPane>
  </children>

                              <TextField fx:id="havemoney" alignment="CENTER" editable="false" layoutX="17.0" layoutY="26.0" mouseTransparent="true" prefHeight="22.0" prefWidth="108.0" />
                           </children>
                        </AnchorPane>
                        <AnchorPane layoutX="1.0" layoutY="146.0" prefHeight="55.0" prefWidth="142.0">
                           <children>
                              <Label fx:id="stock" alignment="CENTER" contentDisplay="CENTER" layoutX="20.0" layoutY="3.0" prefHeight="22.0" prefWidth="102.0" text="보유 주식" textAlignment="CENTER" />
                              <TextField fx:id="havestock" alignment="CENTER" editable="false" layoutX="18.0" layoutY="26.0" mouseTransparent="true" prefHeight="22.0" prefWidth="108.0" />
                           </children>
                        </AnchorPane>
                        <AnchorPane layoutY="204.0" prefHeight="91.0" prefWidth="142.0">
                           <children>
                              <Button fx:id="buy" layoutX="18.0" layoutY="59.0" mnemonicParsing="false" onAction="#selectBuyButton" prefHeight="22.0" prefWidth="49.0" text="매수" />

                        <Label fx:id="getnicknamebox" alignment="CENTER" contentDisplay="CENTER" layoutX="31.0" layoutY="24.0" prefHeight="22.0" prefWidth="102.0" text="닉네임 입력" textAlignment="CENTER" />
                        <TextField fx:id="getnamevalue" alignment="CENTER" layoutX="14.0" layoutY="55.0" mouseTransparent="true" prefHeight="22.0" prefWidth="87.0" />

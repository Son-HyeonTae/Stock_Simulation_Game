                        <AnchorPane layoutX="1.0" layoutY="28.0" prefHeight="55.0" prefWidth="142.0">
                           <children>
                              <Label fx:id="nickname" alignment="CENTER" contentDisplay="CENTER" layoutX="20.0" layoutY="3.0" prefHeight="22.0" prefWidth="102.0" text="닉네임" textAlignment="CENTER" />
                              <TextField fx:id="namevalue" alignment="CENTER" editable="false" layoutX="17.0" layoutY="30.0" mouseTransparent="true" prefHeight="22.0" prefWidth="108.0" />
                           </children>
                        </AnchorPane>
                        <AnchorPane layoutX="1.0" layoutY="88.0" prefHeight="55.0" prefWidth="142.0">

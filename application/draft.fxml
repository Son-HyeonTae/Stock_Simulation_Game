                              <TextField fx:id="stockvalue" alignment="CENTER" layoutX="19.0" layoutY="24.0" prefHeight="22.0" prefWidth="87.0" />
                              <Label fx:id="trade" alignment="CENTER" contentDisplay="CENTER" layoutX="20.0" layoutY="3.0" prefHeight="22.0" prefWidth="102.0" text="주식 거래" textAlignment="CENTER" />
                              <Label fx:id="shares" alignment="CENTER" contentDisplay="CENTER" layoutX="104.0" layoutY="24.0" prefHeight="22.0" prefWidth="25.0" text="주" textAlignment="CENTER" />
                           </children>
                        </AnchorPane>
                     </children>
                  </AnchorPane>
                  <AnchorPane layoutX="203.0" prefHeight="91.0" prefWidth="407.0">
                     <children>
                        <ImageView fx:id="printnews" fitHeight="82.0" fitWidth="411.0" layoutX="14.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </AnchorPane>
                  <AnchorPane layoutX="7.0" layoutY="91.0" prefHeight="309.0" prefWidth="143.0">
                     <children>
                        <Label fx:id="stockitem" alignment="CENTER" layoutX="16.0" layoutY="7.0" prefHeight="25.0" prefWidth="119.0" text="종목" textAlignment="CENTER" />
